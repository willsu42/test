<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="387dp"
            android:layout_height="295dp" app:srcCompat="@drawable/tw2330"
            android:id="@+id/imageView2" android:visibility="visible"
            tools:visibility="visible" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintVertical_bias="0.039"
    />
    <TextView
            android:text="股價圖"
            android:textSize="24sp"
            android:layout_width="122dp"
            android:layout_height="29dp"
            android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"/>

    <ImageView
            android:layout_width="350dp"
            android:layout_height="300dp" app:srcCompat="@drawable/predict2330"
            android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintBottom_toBottomOf="parent"
    />
    <TextView
            android:text="預測圖"
            android:textSize="24sp"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/textView2"
            android:visibility="visible"
            tools:visibility="visible"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView4"/>

</androidx.constraintlayout.widget.ConstraintLayout>